      <Button fx:id="back" layoutX="24.0" layoutY="345.0" mnemonicParsing="false" onAction="#backToLobby" prefHeight="25.0" prefWidth="44.0" text="back" />
      <TextField fx:id="lobbyTitle" layoutX="226.0" layoutY="175.0" promptText="Title" />
      <ChoiceBox fx:id="gameChoice" layoutX="226.0" layoutY="214.0" prefWidth="150.0" />
      <Label alignment="CENTER" layoutX="226.0" layoutY="55.0" prefHeight="72.0" prefWidth="150.0" text="Create match">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="create" layoutX="227.0" layoutY="271.0" mnemonicParsing="false" onAction="#createLobby" prefHeight="36.0" prefWidth="149.0" text="Create" />
   </children>
</AnchorPane>
